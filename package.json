{
  "name": "Ourchat",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "dev": "react-native run-ios --simulator=\"iPhone 7\" && open \"rndebugger://set-debugger-loc?host=localhost&port=8081\"",
    "test": "jest",
    "clean:all": "yarn clean:cache && yarn clean:builds && yarn clean:dependencies",
    "clean:cache": "rm -rf $TMPDIR/react-*; rm -rf $TMPDIR/haste-*; rm -rf $TMPDIR/metro-*; watchman watch-del-all",
    "clean:build:android": "rm -rf ./android/build; rm -rf ./android/.gradle",
    "clean:build:ios": "rm -rf ./ios/build",
    "clean:builds": "yarn clean:build:android && yarn clean:build:ios",
    "clean:dependencies": "rm -rf ./node_modules; rm yarn.lock"
  },
  "dependencies": {
    "react": "16.6.1",
    "react-native": "0.57.7",
    "react-native-gesture-handler": "^1.0.10",
    "react-navigation": "^3.0.5",
    "react-navigation-redux-helpers": "^2.0.8",
    "react-redux": "^6.0.0",
    "redux": "^4.0.1"
  },
  "devDependencies": {
    "babel-jest": "23.6.0",
    "jest": "23.6.0",
    "metro-react-native-babel-preset": "0.50.0",
    "react-test-renderer": "16.6.1"
  },
  "jest": {
    "preset": "react-native"
  }
}
